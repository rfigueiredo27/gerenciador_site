<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	
	<display-name>Composicao</display-name>

	<!-- Declaracao do servlet do Spring MVC abaixo-->
	
	<servlet>
		<servlet-name>GravarXML</servlet-name>
		<servlet-class>br.jus.trerj.controle.composicao.GravaXML</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GravarXML</servlet-name>
		<url-pattern>/gravaxml</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>LerXML</servlet-name>
		<servlet-class>br.jus.trerj.controle.composicao.LerXML</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LerXML</servlet-name>
		<url-pattern>/lerXML</url-pattern>
	</servlet-mapping>
	
</web-app>
